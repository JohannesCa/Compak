#!/bin/bash

declare -a files=("generated.fib25" "generated.equal")

echo "...Cleaning output dir"
rm -f output/*

for i in "${files[@]}"
do
   echo "...Testing compression with file $i"
   ./Compak-3.py $i
   echo ""
   echo "...Comparing input file and generated file (diff)"
   diff -s files/$i output/$i
   echo ""
done

echo "...Testing done"
